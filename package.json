{
  "name": "@icalialabs/run-ci-compose",
  "version": "0.0.1",
  "description": "Runs Docker Compose using an additional config file called 'ci-compose.yml'",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "scripts": {
    "debug": "ts-node -r dotenv/config src/main.ts",
    "build": "tsc && ncc build",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest --coverage",
    "pre-checkin": "yarn run format && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icalia-actions/run-ci-compose.git"
  },
  "keywords": [
    "actions",
    "icalia",
    "docker",
    "docker-compose",
    "build",
    "ci"
  ],
  "author": "Icalia Labs",
  "contributors": [
    {
      "name": "Vov",
      "url": "https://ical.ink/vov"
    }
  ],
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/tmp": "^0.2.0",
    "@vercel/ncc": "^0.27.0",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "jest-runtime": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "typescript-formatter": "^7.2.2",
    "yaml": "^1.10.2"
  }
}
